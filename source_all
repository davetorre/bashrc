. $HOME/.bash/git-completion
. $HOME/.bash/colors
. $HOME/.bash/environment_setup
. $HOME/.bash/prompt
. $HOME/.bash/aliases

BASH_COMPLETION=$HOME/.bash/bash_completion
if [ -f $BASH_COMPLETION ]; then
   . $BASH_COMPLETION
fi

shopt -s autocd

#KEEP AT END FOR RVM
[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm"